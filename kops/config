apiVersion: kops/v1alpha2
kind: Cluster
metadata:
  name: mycluster.wavetesting.com
spec:
  cloudProvider: aws
  configBase: s3://kub-wavetesting-com-state-store/mycluster.wavetesting.com
  etcdClusters:
  - etcdMembers:
    - instanceGroup: master-eu-west-1a
      name: a
    name: main
  - etcdMembers:
    - instanceGroup: master-eu-west-1a
      name: a
    name: events
  masterInternalName: api.internal.mycluster.wavetesting.com
  masterPublicName: api.mycluster.wavetesting.com
  subnets:
  - cidr: 172.20.32.0/19
    name: eu-west-1a
    type: Public
    zone: eu-west-1a
  topology:
    dns:
      type: Public
    masters: public
    nodes: public

